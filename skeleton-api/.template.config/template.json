{
  "$schema": "http://json.schemastore.org/template",
  "author": "Aleksei Mialkin",
  "classifications": [
    "Skeleton",
    "Web"
  ],
  "identity": "Skeleton.Api",
  "name": "Skeleton Web API",
  "shortName": "skeleton-api",
  "sourceName": "Skeleton",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "api-port": {
      "type": "parameter",
      "datatype":"int",
      "replaces": "API-PORT",
      "isRequired": true
    },
    "postgres-port": {
      "type": "parameter",
      "datatype":"int",
      "replaces": "POSTGRES-PORT",
      "isRequired": true
    },
    "main-model-name": {
      "type": "parameter",
      "datatype":"text",
      "replaces": "UserX",
      "fileRename": "UserX",
      "isRequired": true
    },
    "main-model-name-lowercase": {
      "type": "derived",
      "valueSource": "main-model-name",
      "replaces": "userX",
      "valueTransform": "firstLowerCaseInvariant"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "**/.env",
            "**/.git/**",
            "**/.idea/**",
            "**/.DS_Store"
          ]
        }
      ]
    }
  ]
}
